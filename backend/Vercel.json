{
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "build/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/products",
      "dest": "index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/categories",
      "dest": "index.js"
    },
    {
      "src": "/categories/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/brands",
      "dest": "index.js"
    },
    {
      "src": "/brands/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/ratingRanges",
      "dest": "index.js"
    },
    {
      "src": "/ratingRanges/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/priceRanges",
      "dest": "index.js"
    },
    {
      "src": "/priceRanges/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/users",
      "dest": "index.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/auth",
      "dest": "index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/cart",
      "dest": "index.js"
    },
    {
      "src": "/cart/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/favorite",
      "dest": "index.js"
    },
    {
      "src": "/favorite/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/orders",
      "dest": "index.js"
    },
    {
      "src": "/orders/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "build/$1"
    }
  ]
}
